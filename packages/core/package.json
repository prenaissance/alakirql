{
  "name": "@alakir/core",
  "type": "module",
  "version": "1.0.0",
  "description": "DSL for astrology",
  "files": [
    "dist"
  ],
  "exports": {
    "./lexer": {
      "import": "./dist/lexer/index.js",
      "require": "./dist/lexer/index.cjs",
      "types": "./dist/lexer/index.d.ts"
    },
    "./parser": {
      "import": "./dist/parser/index.js",
      "require": "./dist/parser/index.js.cjs",
      "types": "./dist/parser/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src --dts --format esm,cjs --external config --clean",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "dsl",
    "astrology",
    "lexer",
    "parser",
    "interpreter"
  ],
  "author": "prenaissance",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@vitest/coverage-c8": "^0.29.1",
    "config": "workspace:*",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2"
  }
}